"""
Calculating mode
"""

from collections import Counter


def calc_mode(numbers):
    return Counter(numbers)
        
def main():
    target=[1,4,5,6,2,4,6,2,4,3,1,3,5,2,5,2,2,3,2,3,5,2,4,5,2,3,4,2,4]
    mode=calc_mode(target)
    print("mode=",mode)
    top=1
    print("most common=",mode.most_common(top))

if __name__=="__main__":
    main()